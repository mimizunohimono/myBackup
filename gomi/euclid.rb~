
def euclid(a, b)
  x1 = 1
  y1 = 0
  z1 = a
  x2 = 0
  y2 = 1
  z2 = b

  while(z2 != 1) do
    break if z2 == 0
    q = (z1 - (z1 % z2)) / z2    
    x1 = x1 - (q*x2)
    y1 = y1 - (q*y2)
    z1 = z1 - (q*z2)
    x1, x2 = x2, x1
    y1, y2 = y2, y1
    z1, z2 = z2, z1
  end
  
  if y2 < 0
    return y2 + a
  else return y2
  end
end


N = 
a = 24325865027739343245883524441153607722099475929785543376822181414689070913408551436101767561143493080166203404800

p e = euclid(N, a)
p a*e%N

